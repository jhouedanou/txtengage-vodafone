{
    "version": 2,
    "buildCommand": "npm run build",
    "outputDirectory": ".output",
    "installCommand": "npm install",
    "framework": "nuxt",
    "routes": [
        {
            "src": "/images/(.*)",
            "headers": { "cache-control": "public, max-age=31536000, immutable" },
            "dest": "/images/$1"
        },
        {
            "handle": "filesystem"
        },
        {
            "src": "/(.*)",
            "status": 404,
            "dest": "/_nuxt/404.html"
        }
    ]
}