{
    "version": 2,
    "builds": [
        {
            "src": "package.json",
            "use": "@vercel/static-build",
            "config": {
                "buildCommand": "yarn build",
                "outputDirectory": ".output/public",
                "installCommand": "yarn install"
            }
        }
    ],
    "routes": [
        {
            "src": "/images/(.*)",
            "dest": "/images/$1",
            "continue": true
        },
        {
            "handle": "filesystem"
        },
        {
            "src": "/(.*)",
            "dest": "/"
        }
    ]
}