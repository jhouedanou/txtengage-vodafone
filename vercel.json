{
    "version": 2,
    "builds": [
        {
            "src": "package.json",
            "use": "@vercel/static-build",
            "config": {
                "buildCommand": "npm run build",
                "outputDirectory": ".output/public",
                "installCommand": "npm install"
            }
        }
    ],
    "routes": [
        {
            "src": "/images/(.*)",
            "headers": { "cache-control": "public, max-age=31536000, immutable" },
            "dest": "/images/$1"
        },
        {
            "handle": "filesystem"
        },
        {
            "src": "/(.*)",
            "dest": "/"
        }
    ],
    "functions": {
        "__nitro": {
            "runtime": "nodejs20.x"
        }
    }
}